{
    "name": "gsposato/yii2-audit",
    "description": "Yii2 Audit records and displays web/cli requests, database changes, php/js errors and associated data.",
    "keywords": ["yii2", "audit", "trail", "logging"],
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "authors": [
        {
            "name": "Steve Guns",
            "homepage": "http://gsposato.com/",
            "email": "Steve@gsposato.com"
        },
        {
            "name": "Brett O'Donnell",
            "homepage": "https://mrphp.com.au/",
            "email": "cornernote@gmail.com"
        },
        {
            "name": "Marc Hollander",
            "homepage": "http://maruc.nl/",
            "email": "marchollander@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-bcmath": "*",
        "yidas/yii2-bower-asset": "~2.0.6",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-debug": "~2.0.0",
        "phpspec/php-diff": "*"
    },
    "autoload": {
        "psr-4": {"gsposato\\yii2\\audit\\": "src"}
    },
    "extra": {
        "bootstrap": "gsposato\\yii2\\audit\\Bootstrap"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gsposato/yii2-bower-asset.git"
        }
    ]
}
